    "src/docs/email/Exchange.json",
    "src/docs/email/imapsmtp.json",
    "src/docs/excel/application.json",
    "src/docs/excel/files.json",
    "src/docs/outlook/application.json",
    "src/docs/robocloud/items.json",
    "src/docs/robocloud/secrets.json",
    "src/docs/word/application.json"
